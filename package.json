{
  "name": "job-server",
  "version": "0.0.1",
  "description": "",
  "main": "target",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc mocha test/**/*.{ts,tsx}",
    "build": "rm -rf target && tsc",
    "format": "tslint 'src/**/*.ts' --fix",
    "lint": "tslint 'src/**/*.ts'",
    "start": "node target"
  },
  "nyc": {
    "all": true,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "web/**"
    ],
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "author": "Grzegorz Abramczyk <abramczykg@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@agrzes/jira-adapter": "^0.0.2",
    "@types/moment": "^2.13.0",
    "axios": "^0.18.0",
    "debug": "^4.0.1",
    "express": "^4.16.3",
    "http-link-header": "^1.0.0",
    "js-yaml": "^3.12.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "node-fetch": "^2.3.0",
    "ouch-rx": "^2.0.2",
    "pouchdb-adapter-http": "^7.0.0",
    "pouchdb-core": "^7.0.0",
    "pouchdb-mapreduce": "^7.0.0",
    "pouchdb-replication": "^7.0.0",
    "rxjs": "^6.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/express": "^4.16.0",
    "@types/js-yaml": "^3.11.2",
    "@types/lodash": "^4.14.116",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.9.4",
    "@types/node-fetch": "^2.1.4",
    "@types/pouchdb-adapter-http": "^6.1.2",
    "@types/pouchdb-core": "^7.0.0",
    "@types/pouchdb-replication": "^6.4.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  }
}
